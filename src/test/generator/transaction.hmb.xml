<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="test.beans.Transaction" table="mydb.transaction">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="coin" type="java.lang.String">
			<column name="coin" />
		</property>
		<property name="price" type="java.lang.Double">
			<column name="price" />
		</property>	
		<property name="quantity" type="java.lang.Double">
			<column name="quantity" />
		</property>
		<property name="sell" type="java.lang.Boolean">
			<column name="sell" />
		</property>	
		<property name="buy" type="java.lang.Boolean">
			<column name="buy" />
		</property>	
		<property name="username" type="java.lang.String">
			<column name="username" />
		</property>			
	</class>
</hibernate-mapping>